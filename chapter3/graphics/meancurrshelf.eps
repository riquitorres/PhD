%gri:
%gri:show "++++++++++++++++++++++ drawn axes ++++++++++++++++++++++"
%gri:
%gri:// Loop around no of graphs
%gri:// Set loop parameter
%gri:.loop. = 1
%gri:.nloops. = {rpn .noofgrphs. 1 +}
%gri:while {rpn .loop. .nloops. >}
%gri:
%gri:show "Entered loop " .loop. " of " .noofgrphs.
%gri:
%gri:// Read line and symbol parameters
%gri:read .lwidth. \lcolor .ndash.
%gri:read .symb. .ssize. \scolor .decint.
%gri:
%gri:if ..debug..
%gri:show "Line and symbol parameters \lcolor \scolor"
%gri:end if
%gri:
%gri:// Open data file
%gri:read \data
%gri:read \xcol
%gri:read \ycol
%gri:
%gri:open "\data"
%gri:show "reading ctd data \data"
%gri:
%gri:// FOr Sea-Bird data files
%gri:
%gri://\header = "1"
%gri://while \header
%gri://  read \testnum
%gri://  if { "\testnum" == "*END*"}
%gri://   break
%gri://  end if
%gri://end while
%gri://show "ctd data read "
%gri:
%gri:// For NeilBrown data files
%gri:// watch this space!
%gri:
%gri:read columns x=\xcol y=\ycol   
%gri:close
%gri:x *= 100
%gri:set clip postscript on 
%gri:
%gri://draw curve - none drawn if \lcolor is white or .lwidth. =0
%gri:set line width .lwidth.
%gri:if { "\lcolor" == "greylevel" }
%gri:	set greylevel  { rpn .loop. 1 - .noofgrphs. /  }
%gri:else
%gri:	set color \lcolor
%gri:end if
%gri:set dash .ndash.
%gri:if {rpn 0 .lwidth. >}
%gri:  draw curve
%gri:end if
%gri:set line width default
%gri:set dash off
%gri:set color black
%gri:
%gri://draw symbols - none drawn if .ssize. = 0
%gri:set symbol size .ssize.
%gri:if { "\scolor" == "greylevel" }
%gri:	set greylevel  { rpn  .loop. 1 - .noofgrphs. /  }
%gri:else
%gri:	set color \scolor
%gri:end if
%gri:.kount. = 0
%gri:.kdec. = 0
%gri:.nx. = {rpn ..num_col_data.. 1 -}
%gri:while {rpn .kount. .nx. >}
%gri:if {rpn .kdec. .decint. ==}
%gri:.kdec. = 0
%gri:draw symbol .symb. at {rpn x .kount. @} {rpn y .kount. @}
%gri:end if
%gri:.kount. += 1
%gri:.kdec. += 1
%gri:end while
%gri:set color black
%gri:
%gri:set clip postscript off
%gri:
%gri:// clean out re-read synonyms
%gri:delete \lcolor \scolor 
%gri:
%gri:.loop. += 1
%gri: 
%gri:end while
