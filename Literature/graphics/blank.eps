%gri://interp.gri to use optimal interpolation on incomplete grids
%gri:// it also allows you to mask a region provided the data from a datafile.
%gri:
%gri:expecting version 2.010
%gri:
%gri:                                                   
%gri:// Define data file
%gri:
%gri:\FILE_DATA = "interp.con"
%gri:query \fdata "Data file?  " ("\FILE_DATA")
%gri:
%gri:open \fdata
%gri:
%gri:skip 5
%gri:
%gri:// orientation
%gri:
%gri:
%gri:// Set up the x axis
%gri:
%gri:read .xmin.
%gri:read .xmax.
%gri:read .xinc.
%gri:skip 1
%gri:
%gri:// Set up the y axis
%gri:
%gri:read .ymin.	//bottom depth
%gri:read .ymax.	//surface
%gri:read .yinc.	//increment
%gri:skip 1
%gri:
%gri:// DRAW CONTOURS 
%gri:
%gri:read \contdraw
%gri:read \contfile
%gri:read \xcol \ycol \zcol  
%gri:read \gmethod		// conversion to grid method
%gri:read .xr. .yr. 		// x and y scale parameters
%gri:read .n. .e.	        // min no of pst & extnsn of neighbrhd by  root2^e
%gri:skip 1
%gri:
%gri://SETTING SURFACE-BOTTOM BOUNDARIES FOR THE CONTOUR PLOT
%gri:
%gri:read \maskdraw
%gri:read \maskfile
%gri:read \xcolum \ycolum
%gri:skip 1
%gri:read \gridoutfile
%gri:
%gri:
%gri:close
%gri:
%gri:
%gri:// OPEN DATA FILE
%gri:
%gri:// DRAW CONTOURS
%gri:
%gri:if {"\contdraw" == "yes"}
%gri:
%gri:set missing value 999.99
%gri:open \contfile
%gri:read columns x=\xcol y=\ycol z=\zcol
%gri:close
%gri://if {\zcol == "4"}
%gri://z -= 1
%gri://end if
%gri:show columns statistics
%gri:.xstep. = .xinc.
%gri:.ystep. = .yinc.
%gri:
%gri:set x grid {rpn x min} {rpn x max} .xstep.
%gri:set y grid {rpn y min} {rpn y max} .ystep.
%gri:
%gri:if {"\gmethod" == "default"}
%gri:	convert columns to grid
%gri:else if {"\gmethod" == "objective"}
%gri:	show "objective interpolation"
%gri:	convert columns to grid objective .xr. .yr. .n. .e.
%gri:else if {"\gmethod" == "barnes"}
%gri:	convert columns to grid barnes .xr. .yr. .n. .e.
%gri:
%gri:else if {"\gmethod" == "boxcar"}
%gri:	convert columns to grid boxcar .xr. .yr. .n. .e.
%gri:else
%gri:open /homedir/oss02a/gridding/x.dat
%gri:read grid x 
%gri:open /homedir/oss02a/gridding/y.dat
%gri:read grid y 
%gri:open \gmethod
%gri:read grid data
%gri:close
%gri:end if
%gri:// DRAW MASKING
%gri:if {"\maskdraw" == "yes"}
%gri:	open \maskfile
%gri:	read columns x=\xcolum y=\ycolum
%gri:	set grid missing inside curve
%gri:	close
%gri:end if
%gri:convert grid to columns
%gri:write columns to \gridoutfile 
%gri:
%gri:show "--------------------- Interpolation completed --------------------------"
%gri:quit        
showpage
